            targetNamespace="http://www.example.com/aircraft"
            xmlns="http://www.example.com/aircraft">
    <sch:include schemaLocation="aircraftsType.xsd" />
    <sch:element name="aircrafts" type="stListType"/>

    <sch:complexType name="stListType">
        <sch:sequence>
            <sch:element name="aircraft" type="aircraftsType" minOccurs="1" maxOccurs="unbounded"/>
